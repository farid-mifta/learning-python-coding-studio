#list
list_siswa = ["bryan", "ismi", "riana", "hafiz"]
print(list_siswa)

# Merubah isi dari list (mutable)

# menambah isi dari list
# .append() -> menambah di akhir list
list_siswa.append("artiko")
print("isi list setelah udin ditambah", list_siswa)

# .insert() -> menambah di posisi tertentu
list_siswa.insert(1, "kamilia")
print("isi list setelah lia ditambah", list_siswa)

# mengurangi isi dari list
# .remove()-> hapus berdasarkan nilai/data
list_siswa.remove("riana")
print("isi list siswa setelah di hapus", list_siswa)

# .pop() -> hapus berdasarkan posisi
list_siswa.pop(0)
print("isi list siswa setelah di poo", list_siswa)

# List indexing -> mengubah dan mengakses

# mengakses list
print("posisi dari 3 list :", list_siswa[3])

# merubah isi dari list
list_siswa[2] = "farid"
print("list siswa setelah di rubah", list_siswa)

# slicing di list
# start:stop
# akses artiko dan farid
print("dari nama :", list_siswa [2:4])

# duplikat nama kamilia
list_siswa.insert(3, "kamilia")
print("setelah di duplicate", list_siswa)
print(list_siswa.count("kamilia"))
list_siswa = set(list_siswa)
print(list_siswa)
print("setelah type dirubah", type (list_siswa))
